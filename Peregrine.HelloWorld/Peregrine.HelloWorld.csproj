<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>Peregrine Greeter</Title>
        <Description>A simple greeter package - mostly made to test a NuGet workflow</Description>
        <PackageProjectUrl>https://github.com/eperegrine/Peregrine.HelloWorld</PackageProjectUrl>
        <RepositoryUrl>https://github.com/eperegrine/Peregrine.HelloWorld</RepositoryUrl>
        <RepositoryType>Git</RepositoryType>
        <PackageTags>PoC,Greeter</PackageTags>
        <Authors>Emily Peregrine</Authors>
    </PropertyGroup>

</Project>
